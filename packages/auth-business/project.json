{
  "name": "auth-business",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/auth-business/src",
  "projectType": "library",
  "tags": ["scope:shared", "type:auth"],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/auth-business",
        "main": "packages/auth-business/src/index.ts",
        "tsConfig": "packages/auth-business/tsconfig.lib.json",
        "assets": ["packages/auth-business/*.md"]
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/auth-business/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/auth-business/jest.config.ts"
      }
    }
  }
}
