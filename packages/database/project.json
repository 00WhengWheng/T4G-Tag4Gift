{
  "name": "database",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/database/src",
  "projectType": "library",
  "tags": ["scope:shared", "type:database"],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "tsc --project packages/database/tsconfig.json"
      }
    },
    "generate": {
      "executor": "nx:run-commands",
      "options": {
        "command": "prisma generate --schema=packages/database/schema.prisma"
      }
    },
    "migrate:dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "prisma migrate dev --schema=packages/database/schema.prisma"
      }
    },
    "migrate:deploy": {
      "executor": "nx:run-commands",
      "options": {
        "command": "prisma migrate deploy --schema=packages/database/schema.prisma"
      }
    },
    "studio": {
      "executor": "nx:run-commands",
      "options": {
        "command": "prisma studio --schema=packages/database/schema.prisma"
      }
    }
  }
}
