{
  "name": "types",
  "type": "library",
  "sourceRoot": "src",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/types",
        "main": "src/index.ts",
        "tsConfig": "packages/types/tsconfig.lib.json",
        "assets": ["packages/types/*.md"]
      }
    }
  }
}
