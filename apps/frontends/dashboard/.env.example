# T4G Business Dashboard Environment Configuration (t4g.space)

# Auth0 Business Application Configuration
VITE_AUTH0_BUSINESS_DOMAIN=your-business-tenant.auth0.com
VITE_AUTH0_BUSINESS_CLIENT_ID=your-business-client-id
VITE_AUTH0_BUSINESS_AUDIENCE=https://api.t4g.space

# Business API Configuration
VITE_API_BASE_URL=https://api.t4g.space
VITE_API_VERSION=v1

# Platform Configuration
VITE_PLATFORM=business
VITE_DOMAIN=t4g.space
VITE_APP_NAME=T4G Business Dashboard

# Development URLs (using ngrok or local setup)
# VITE_AUTH0_BUSINESS_DOMAIN=dev-t4g-business.us.auth0.com
# VITE_API_BASE_URL=https://business-api.ngrok.io

# Feature Flags for Business Dashboard
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_VENUE_MANAGEMENT=true
VITE_ENABLE_CHALLENGE_CREATION=true
VITE_ENABLE_REVENUE_TRACKING=true

# Third-party Integrations
VITE_GOOGLE_MAPS_API_KEY=your-google-maps-key
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_your-stripe-key

# Monitoring & Analytics
VITE_SENTRY_DSN=https://your-sentry-dsn@sentry.io/project-id
VITE_GA_TRACKING_ID=GA-XXXXXXXXX
